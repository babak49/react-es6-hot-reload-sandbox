{
  "name": "react-es6-hot-reload-sandbox",
  "version": "1.0.0",
  "description": "ES6, React, Hot Reload, Dev server",
  "main": "./dist/index.js",
  "scripts": {
    "init": "npm install",
    "start": "npm run clean; webpack",
    "start:hard": "rm -rf ./node_modules; npm run init; npm run start",
    "watch": "webpack-dev-server",
    "clean": "rm -rf ./example/build",
    "prepublish": "rm -f ./index.js; webpack --config webpack.config.dist.js"
  },
  "author": "Bogdan Bodnaruk",
  "contributors": [
    "Bogdan Bodnaruk <babak.47031@gmail.com>"
  ],
  "repository": "https://github.com/babak49/react-es6-hot-reload-sandbox",
  "license": "ISC",
  "dependencies": {
    "react": "15.1.0",
    "react-dom": "15.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel": "^6.3.13",
    "babel-core": "^6.3.13",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "file-loader": "^0.9.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "npmName": "react-es6-hot-reload-sandbox",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
